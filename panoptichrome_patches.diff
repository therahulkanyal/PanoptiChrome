+      Handle<SimpleNumberDictionary>((Address*)rk_ast_obj_map->get(fp));
+
+  if (per_function_ast_obj_map.is_null()) {
+#ifdef DEBUG
+    std::cout << "\nPer function AST Object Map is NULL";
