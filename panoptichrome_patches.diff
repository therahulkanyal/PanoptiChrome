+void PersistentHandles::ReplacePtr(Address original, Address new_addr) {
+  // std::cout << "\n\nPTL::ContainsPtr" << loc << std::endl;
+
+  // search if ptr unordered set contains loc
+
+  if (ptrs_.find(original) == ptrs_.end()) return;
