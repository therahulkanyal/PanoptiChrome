+  Scope* scope() const { return scope_; }
+
+  void AddEdge(AstNode* from, AstNode* to) {
+#ifdef DEBUG
+    if (true) {
+      std::cout << "\nEdge added from " << from->position() << " :: type :: ";
+      printType(from->node_type());
